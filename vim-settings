"Sudhir
set nocompatible
set backspace=indent,eol,start
syntax on
filetype plugin indent on
" colorscheme solarized
if has('gui_running')
        set background=light
else
        "set background=dark
        set colorscheme evening
endif
let html_use_css=1

set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set hlsearch
set ignorecase
set nopaste
set smartcase
set incsearch
" set autoindent
" set nowrap
command! Q q "Bind :Q to :q
